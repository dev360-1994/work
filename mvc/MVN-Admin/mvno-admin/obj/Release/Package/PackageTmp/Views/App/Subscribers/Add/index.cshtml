<script>
    $(document).ready(function () {

        // Tooltip Trigger
        const tooltipTriggerList = document.querySelectorAll('[data-bs-toggle="tooltip"]')
        const tooltipList = [...tooltipTriggerList].map(tooltipTriggerEl => new bootstrap.Tooltip(tooltipTriggerEl))

        // Select all DatePicker elements on the page
        $("input[data-role='datepicker']").click(function () {
            var datePicker = $(this).data("kendoDatePicker");
            if (datePicker) {
                datePicker.open();
            }
        });

        // Create the Progress Bar
        $("#mvno-admin-step-progress").kendoCircularProgressBar({
            min: 0,
            max: 100,
            value: 0,
            animation: false,
            centerTemplate: '<div><span>#= value #%</span></div>',
        });

        // On window resize, redraw the progress bar to 120px
        $(window).on('resize', (e) => {
            $("#ap-step-progress").css({ width: "120px", height: "120px" })
                .data("kendoCircularProgressBar").resize();
        })

        // Hide all step section except first on page load
        $(".mvno-admin-step-section:first-child").removeClass("d-none").addClass("d-flex");
        $("#mvno-admin-steps li:first-child").addClass("mvno-admin-step-active");

        // Handle the "checkAll" checkbox change event
        $('.check-all').change(function () {
            var checked = $(this).is(':checked');
            $('.checkbox').prop('checked', checked);
        });

        // Handle individual checkbox changes
        $('.checkbox').change(function () {
            var allCheckboxes = $('.checkbox');
            var allChecked = allCheckboxes.length === allCheckboxes.filter(':checked').length;
            $('.check-all').prop('checked', allChecked);
        });


        /* Temporary Logic */

        // Eligibility Page
        toggleEligibility = () => {
            $("#mvno-admin-not-eligible").hide();
            $("#mvno-admin-eligible").toggleClass('d-none');
        }

        // Enroll Page
        activateDevice = () => {
            $('#mvno-admin-order-details').removeClass('d-none');
            $('#mvno-admin-complete-application').removeAttr('disabled')
        }
    });

    showStep = (stepindex, stepProgress, trigger) => {
        $("#mvno-admin-steps li").removeClass("mvno-admin-step-active");
        $(".mvno-admin-step-section").removeClass('d-flex').addClass("d-none");
        $(".mvno-admin-step-section:nth-child(" + stepindex + ")").removeClass('d-none').addClass("d-flex");
        $("#mvno-admin-steps li:nth-child(" + stepindex + ")").addClass("mvno-admin-step-active");
       
        // Step Progress
        $("#mvno-admin-step-progress").data("kendoCircularProgressBar").value(stepProgress);

    }
</script>